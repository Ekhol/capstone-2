{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
        "title": {
            "type": "string",
            "minLength": 1
        },
        "postText": {
            "type": "string",
            "minLength": 1
        },
        "template": {
            "type": "string"
        },
        "userId": {
